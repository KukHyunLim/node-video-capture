    "type": "git",
    "url": "git+https://github.com/xan105/node-video-capture.git"
  },
  "bugs": {
    "url": "https://github.com/xan105/node-video-capture/issues"
  },
  "homepage": "https://github.com/xan105/node-video-capture#readme",
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.15.0",
    "prettier": "2.2.0"
